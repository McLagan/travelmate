# Rate Limiting (generous for development)
RATE_LIMIT_SEARCH=50
RATE_LIMIT_ROUTES=100
RATE_LIMIT_AUTH=20

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s